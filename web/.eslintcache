ox>\n            {navRoutes.map(({ label, href, icon }) => (\n              <Button\n                startIcon={icon}\n                key={label}\n                to={href}\n                className={button}\n                component={Link}\n              >\n                {label}\n              </Button>\n            ))}\n\n            {loading \n            ? (\n              <PulseLoader color='#7352d0' loading={loading} size={10}/> \n            ) \n            : (\n              user\n                ? (\n                  <>\n                    <Button \n                      variant='contained' \n                      color='secondary' \n                      to='/b'\n                      component={Link}\n                      className={actionButtons}\n                    >\n                      Tus Tableros\n                    </Button>\n                    <Button \n                      variant='contained' \n                      color='secondary' \n                      className={actionButtons}\n                      onClick={removeUser}\n                    >\n                      Cerrar sesión\n                    </Button>\n                  </>\n                )\n                : (\n                  <>\n                    <Button \n                      variant='contained' \n                      color='secondary' \n                      to='/signin'\n                      component={Link}\n                      className={actionButtons}\n                    >\n                      Inicio de sesión\n                    </Button>\n\n                    <Button \n                      variant='contained' \n                      color='secondary'\n                      to='/signup'\n                      component={Link}\n                      className={actionButtons}\n                    >\n                      Registro\n                    </Button>\n                  </>\n              )\n            )}\n          </Box>\n        </