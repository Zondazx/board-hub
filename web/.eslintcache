lette.secondary.main\n    },\n  },\n}));\n\nconst navRoutes = [\n  {\n    label: 'Acerca de nosotros',\n    href: '/about',\n    icon: <InfoIcon />,\n  },\n  {\n    label: 'Contacto',\n    href: '/contact',\n    icon: <ContactPhoneIcon />,\n  },\n  {\n    label: 'Open Source',\n    href: '/opensource',\n    icon: <GitHubIcon />,\n  },\n];\n\nconst Nav = ({ location }) => {\n  const { \n    logo,\n    header, \n    toolbar, \n    button, \n    actionButtons \n  } = useStyles();\n  const {\n    user,\n    loading,\n    setUser,\n    removeUser,\n  } = useUserStore();\n  const token = localStorage.getItem('token');\n\n  useEffect(() => {\n    if (!user && token) {\n      setUser();\n    }\n  }, []);\n\n  return (\n    <>\n      <AppBar className={header}>\n        <Toolbar className={toolbar}>\n          <Link to={'/'}>\n            <img src={logoImage} className={logo} width='160px' alt='Logo' />\n          </Link>\n\n          <Box>\n            {navRoutes.map(({ label, href, icon }) => (\n              <Button\n                startIcon={icon}\n                key={label}\n                to={href}\n                className={button}\n                component={Link}\n              >\n                {label}\n              </Button>\n            ))}\n\n            {loading \n            ? (\n              <PulseLoader color='#7352d0' loading={loading} size={10}/> \n            ) \n            : (\n              user\n                ? (\n                  <>\n                    <Button \n                      variant='contained' \n                      color='secondary' \n                      to='/b'\n                      component={Link}\n                      className={actionButtons}\n                    >\n                      Tus Tableros\n                    </Button>\n                    <Button \n                      variant='contained' \n                      color='secondary' \n                      className={actionButtons}\n                      onC